<%= simple_form_for(@wage, html: { class: "form-horizontal"}) do |f| %>
  <%= f.error_notification %>

  <%= f.input :title %>
  <%= f.input :company %>
  <%= f.association :location, label_method: :name, value_method: :id %>
  <%= f.input :total %>
  <%= f.input :avg %>
  <%= f.input :high %>
  <%= f.input :low %>

  <div class="form-actions">
    <%= f.button :submit, class: "btn btn-primary" %>
  </div>
<% end %>
